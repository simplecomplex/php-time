    "keywords": ["utils","time","datetime","PHP"],
    "homepage": "https://github.com/simplecomplex/php-time",
    "license": "MIT",
    "authors": [
        {
            "name": "Jacob Friis Mathiasen",
            "email": "jacob.friis@simplecomplex.net"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-ctype": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "suggest": {
        "simplecomplex/inspect": "Better variable dumps and trace."
    },
    "autoload": {
        "psr-4": {
            "SimpleComplex\\Time\\": "src/",
            "SimpleComplex\\Tests\\Time\\": "tests/src/"
        }
